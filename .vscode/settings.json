{
  "javascript.validate.enable": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "vue",
    "jsx",
    "tsx",
    "ts",
    "typescript",
    "html",
    "javascript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2286b6",
    "activityBar.background": "#2286b6",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#861963",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#2286b6",
    "statusBar.background": "#1a668b",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2286b6",
    "statusBarItem.remoteBackground": "#1a668b",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#1a668b",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1a668b99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1a668b"
}
